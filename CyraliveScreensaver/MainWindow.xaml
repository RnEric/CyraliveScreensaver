<Window x:Class="CyraliveScreensaver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CyraliveScreensaver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MouseMove="Window_MouseMove" WindowStyle="None" WindowState="Maximized" ShowInTaskbar="False" Cursor="None" KeyUp="Window_KeyUp">
    <Grid>
        <Canvas x:Name="Diphylleia_snowfall" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
        <TextBlock x:Name="Cyralive_Screen_Clock" HorizontalAlignment="Left" Margin="30,45,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Foreground="White"/>
    </Grid>
</Window>
